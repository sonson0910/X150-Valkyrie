{
  "name": "valkyrie-cardano-wallet",
  "version": "1.0.0",
  "description": "Advanced Cardano Wallet with Enhanced Security and Offline Features",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "eas build",
    "analyze:bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output /tmp/index.android.bundle && npx source-map-explorer /tmp/index.android.bundle",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emurgo/cardano-serialization-lib-browser": "^11.5.0",
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/webpack-config": "~19.0.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@types/react": "~19.0.10",
    "bip39": "^3.1.0",
    "crypto-js": "^4.1.1",
    "expo": "^53.0.20",
    "expo-av": "~15.1.7",
    "expo-clipboard": "~7.1.5",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-local-authentication": "~16.0.5",
    "expo-secure-store": "~14.2.3",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-biometrics": "^3.0.1",
    "react-native-ble-manager": "^10.1.2",
    "react-native-bluetooth-classic": "^1.60.0-rc.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keychain": "^8.1.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-nfc-manager": "^3.14.1",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.11.2",
    "react-native-vector-icons": "^10.0.0",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@testing-library/jest-native": "^5.0.0",
    "@testing-library/react-native": "^12.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "assert": "^2.1.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "buffer": "^6.0.3",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.1",
    "eas-cli": "^16.17.4",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.2.1",
    "process": "^0.11.10",
    "react-test-renderer": "18.2.0",
    "stream-browserify": "^3.0.0",
    "string-replace-loader": "^3.2.0",
    "typescript": "~5.8.3",
    "util": "^0.12.5"
  },
  "overrides": {
    "@expo/config-plugins": "~10.1.1",
    "@expo/prebuild-config": "~9.0.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-bluetooth-classic",
          "react-native-nfc-manager",
          "expo-barcode-scanner"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "keywords": [
    "cardano",
    "wallet",
    "cryptocurrency",
    "blockchain",
    "react-native",
    "mobile",
    "security"
  ],
  "author": "Valkyrie Team",
  "license": "MIT"
}
